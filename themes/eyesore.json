    //"contrastBorder": "#CC4E00"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#000000",
        "background": "#ffffff"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#507a4a"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "",
        "foreground": "#507a4a"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": [
        "comment.documentation",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#507a4a"
      }
    },
    {
      "name": "Invalid - Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#ffcdd2"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "background": "#ffcdd2",
        "foreground": "#000000"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#d132b9"
      }
    },
    {
      "name": "Types",
      "scope": [
        "storage.type",
        "support.type"
      ],
      "settings": {
        "foreground": "#006064"
      }
    },
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#37474f"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c62828"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e65100"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#d500f9"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Numbers, Characters",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant"
      ],
      "settings": {
        "foreground": "#395499"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#c2185b"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#536dfe"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "foreground": "#673ab7"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#ff5722"
      }
    },
    {
      "name": "Embedded Source",
      "scope": [
        "string source",
        "text source"
      ],
      "settings": {
        "background": "#ffab40"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": [
        "meta.tag.sgml.doctype",
        "meta.tag.sgml.doctype string",
        "meta.tag.sgml.doctype entity.name.tag",
        "meta.tag.sgml punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#536dfe"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": [
        "meta.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#536dfe"
      }
    },
    {
      "name": "HTML: Tag Names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#536dfe"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": [
        "meta.tag entity.other.attribute-name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d132b9"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": [
        "constant.character.entity",
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#550fbc"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#d132b9"
      }
    },
    {
      "name": "CSS: Property Names",
      "scope": [
        "meta.property-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#536dfe"
      }
    },
    {
      "name": "CSS: Property Values",
      "scope": [
        "meta.property-value",
        "meta.property-value constant.other",
        "support.constant.property-value"
      ],
      "settings": {
        "foreground": "#165a39"
      }
    },
    {
      "name": "CSS: Important Keyword",
      "scope": "keyword.other.important",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "background": "#ffffff",
        "foreground": "#445ea0"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "background": "#ffffff",
        "foreground": "#822626"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "background": "#ffffff",
        "foreground": "#a71c02"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "background": "#ffffff",
        "foreground": "#1170ba"
      }
    },
    {
      "name": "Markup: Link",
      "scope": "meta.link",
      "settings": {
        "foreground": "#5485df"
      }
    },
    {
      "name": "Markup: Output",
      "scope": [
        "markup.output",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#ed4e4e"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#b6881e"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#498c3e"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#0b6b6f"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#306629"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#643365"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#595603"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#13816f"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": [
        "meta.diff.range",
        "meta.diff.index",
        "meta.separator"
      ],
      "settings": {
        "background": "#d2d2d2",
        "foreground": "#000000"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "background": "#d2d2d2",
        "foreground": "#000000"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "background": "#d2d2d2",
        "foreground": "#000000"
      }
    }
  ]
}
